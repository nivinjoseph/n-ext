{
  "name": "n-ext",
  "version": "1.0.8",
  "description": "Extensions for core Javascript types",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "ts-build": "tsc -p .",
    "x-ts-build-watch1": "npm run ts-build -- -w",
    "ts-build-dist": "tsc -p ./dist",
    "x-ts-build-watch2": "watch 'npm run build' ./src --wait 1 --interval 1",
    "pretest": "npm run ts-build",
    "test": "mocha --reporter spec --ui tdd --require ts-node/register './test/**/*.test.js' || true",
    "pretest-ci": "npm run pretest",
    "test-ci": "mocha --reporter spec --ui tdd --require ts-node/register './test/**/*.test.js'",
    "pregit-publish": "npm run ts-build-dist && git add . && git commit -m 'preparing to publish new version'",
    "git-publish": "npm version patch",
    "postgit-publish": "git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nivinjoseph/n-ext.git"
  },
  "keywords": [
    "extensions"
  ],
  "author": "NiviN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nivinjoseph/n-ext/issues"
  },
  "homepage": "https://github.com/nivinjoseph/n-ext#readme",
  "devDependencies": {
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/sinon": "^1.16.35",
    "mocha": "^3.2.0",
    "nsp": "^2.6.2",
    "sinon": "^2.0.0-pre.6",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "typescript": "2.2.1",
    "watch": "^1.0.2"
  }
}
